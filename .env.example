# ScaffoldX Environment Variables
# Copy this file to .env and update with your settings

# Project root directory (required)
SCAFFOLDX_REPO_ROOT=[ABSOLUTE_FILE_PATH_TO_REPO_ROOT_FOLDER]
REPO_ROOT=[ABSOLUTE_FILE_PATH_TO_REPO_ROOT_FOLDER]

PROJECT_NAME="WhisperDog"
USER="xblabs"

# Environment type (optional)
SCAFFOLDX_ENV=development

# Enable debug output (optional)
SCAFFOLDX_DEBUG=false

# Default AI environment (optional)
SCAFFOLDX_AI_ENV=claude

# API Keys
GEMINI_API_KEY="[MY_KEY]"
OPENAI_API_KEY="[MY_KEY]"
DEEPSEEK_API_KEY="[MY_KEY]"

